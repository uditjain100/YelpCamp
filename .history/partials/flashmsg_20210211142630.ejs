<div class="mt-2">
  <div class="alert alert-success alert-dismissible fade show" role="alert">
    <strong><%= success %></strong>
    <button
      type="button"
      class="close mx-0"
      data-dismiss="alert"
      aria-label="Close"
    >
      <span aria-hidden="true">&times;</span>
    </button>
  </div>
</div>
<% if(success && success.length) {%> <% } %>
